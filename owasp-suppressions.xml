<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Example suppressions -->
    <!-- 
    <suppress>
        <notes>Suppressing false positive for library</notes>
        <gav>group:artifact:version</gav>
        <cve>CVE-YYYY-XXXX</cve>
    </suppress>
    -->
    
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes>Spring Boot managed dependencies are generally safe</notes>
        <gav>org.springframework.boot:*:*</gav>
    </suppress>
    
    <!-- Suppress false positives for test dependencies -->
    <suppress>
        <notes>Test dependencies are not deployed to production</notes>
        <gav>*:*:*</gav>
        <cpe>cpe:/a:test:*:*</cpe>
    </suppress>
</suppressions> 